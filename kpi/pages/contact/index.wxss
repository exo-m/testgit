.contact_wrap {
    width: 100%;
    background-color: #fff;
}
.weui-search-bar {
    position: relative;
    padding: 8px 10px;
    display: flex;
    box-sizing: border-box;
    background-color: #fff;
}
.weui-icon-search {
    margin-right: 8px;
    font-size:inherit;
}
.weui-icon-search_in-box {
    position: absolute;
    left: 10px;
    top: 7px;
}
.weui-search-bar__text{
    display: inline-block;
    font-size: 14px;
    vertical-align: middle;
}
.weui-search-bar__form {
    position: relative;
    flex: auto;
    border-radius: 5px;
    background: #EEEEEE;
    border: 1rpx solid #E6E6EA;
}
.weui-search-bar__box {
    position: relative;
    padding-left: 30px;
    padding-right: 30px;
    width: 100%;
    box-sizing: border-box;
    z-index: 1;
}
.weui-search-bar__input {
    height: 28px;
    line-height: 28px;
    font-size: 14px;
}
.weui-icon-clear {
    position: absolute;
    top: 0;
    right: 0;
    padding: 7px 8px;
    font-size: 0;
}
.weui-search-bar__label {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 2;
    border-radius: 3px;
    text-align: center;
    color: #9B9B9B;
    background: #eee;
    line-height: 28px;
}
.weui-search-bar__cancel-btn {
    margin-left: 10px;
    line-height: 28px;
    color: #09BB07;
    white-space: nowrap;
}
.page__bd {
    position: relative;
}
.searchbar-result{
    margin-top: 0;
    font-size: 16px;
    background-color: #fff;
    position: absolute;
    top: 46px;
    left: 0px;
    width: 100%;
    z-index: 1000;
}
.searchbar-result:before{
    display: none;
}
.res {
    width: 100%;
    height: 40px;
    line-height: 40px;
    padding-left: 30px;
}
.res_bd {
    width: 90%;
    border-bottom: 1px solid #eee;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.list-wrap {
    width: 100%;
    height: -webkit-calc(100vh - 54px);
    height: calc(100vh - 54px);
}
.scrool-wrap {
    height: 100%;
}
.list_item {
    width: 100%;
    border-bottom: 1px solid #eee;
}
.list_item.active {
    border-bottom: none;
}
.list_item .list_header {
    width: 100%;
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    padding-left: 20px;
    position: relative;
}
.icon_arrow {
    position: absolute;
    width: 26px;
    height: 26px;
    top: 12px;
    right: 30px;
    -webkit-transition: all 400ms;
    transition: all 400ms;
}
.icon_arrow.icon_active {
    -webkit-transform: rotateZ(90deg);
    transform: rotateZ(90deg);
}
.weui-cells {
    height: 0px;
    overflow: hidden;
}
.weui-cells.active {
    height: auto;
}
.weui-cell {
    width: 100%;
    height: 70px;
    overflow: hidden;
    display: -webkit-flex;
    display: flex;
    justify-content: flex-start;
    padding: 0px;
    position: relative;
}
.weui-cell_img {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    margin: 15px 10px;
}
.weui-cell_right {
    flex-grow: 2;
    border-bottom: 1px solid #eee;
    padding: 0px;
}
.nums {
    position: absolute;
    font-size: 20px;
    color: #09CA8F;
    top: 0;
    right: 20px;
    height: 70px;
    line-height: 70px;
}
.weui-cell__bd {
    font-size: 20px;
    font-weight: 400;
    color: #333;
    padding: 10px 0 0;
}
.weui-cell__ft {
    color: #999999;
    font-size: 14px;
}