page{
  height: 100%;
  background-color: #f3f3f3;
}

.page-container{
  height: 100%;
  width: 100%;
}

.header{
  width: 100%;
  height: 256rpx;
  background-color: white;
  display: flex;
}

.header-img{
  margin-top: 40rpx;
  width: 160rpx;
  height: 160rpx;
}

.header-description-container{
  flex: 1;
  height: 176rpx;
  margin: 40rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.desrciption-lable{
  display: flex;
  align-items: center;
}

.desrciption-icon{
  font-size: 30rpx;
  color: #cccccc;
}

.desrciption-space{
  width: 20rpx;
}

.desrciption-money{
  vertical-align: bottom;
}

.vouchers-big {
  background-image: url(data:image/png;base64,);
}

.vouchers-small {
  background-image: url(data:image/png;base64,);
}

.cash-coupons-big {
  background-image: url(data:image/png;base64,);
}

.cash-coupons-small {
  background-image: url(data:image/png;base64,);
}

.red-envelopes-small {
  background-image: url(data:image/png;base64,);
}

.discount-coupons-big {
  background-image: url(data:image/png;base64,);
}

.discount-coupons-small {
  background-image: url(data:image/png;base64,);
}

.no-coupon {
  background-image: url(data:image/png;base64,);
}

.assets-container{
  width: 100%;
  height: 320rpx;
  background-color: white;
  display: flex;
}

.assets-left{
  width: 30rpx;
}

.assets-right{
  flex: 1;
  display: flex;
  flex-direction: column;
}

.assets-top{
  height: 99rpx;
  border-bottom: 1rpx #e5e5e5 solid;
  display: flex;
  align-items: center;
}

.assets-bottom{
  flex: 1;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.assets-poiont{
  vertical-align: bottom;
}

.assets-ticket-scroll{
  height: 100rpx;
  width: 720rpx;
  white-space: nowrap;
  display: flex;
}

.assets-ticket{
  margin: 0 10rpx;
  height: 100rpx;
  width: 270rpx;
  display: inline-block;
  color: white;
  position: relative;
}

.assets-ticket-content{
  position:absolute;
  top: 0;
  left: 0;
  height: 100rpx;
  width: 270rpx;
  display: flex;
}

.assets-ticket-left{
  width: 95rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.assets-ticket-right{
  width: 175rpx;
  display: flex;
  flex-direction: column;
  padding-left: 10rpx;
}

.more-ticket-container{
  background-color: white;
  width: 100%;
  padding-bottom: 40rpx;
  padding-top: 10rpx;
}

.more-ticket-content{
  display: flex;
  margin-top: 30rpx;
}

.more-ticket{
  width: 480rpx;
  height: 160rpx;
  display: flex;
  color: white;
}

.more-ticket-left{
  width: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.more-ticket-right{
  width: 360rpx;
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
}

.receive-btn-container{
  flex: 1;
  height: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.receive-btn{
  width: 142rpx;
  height: 52rpx;
  border: 1rpx solid #1a1a1a;
  display: flex;
  align-items: center;
  justify-content: center;
}

.already-receive-btn{
  width: 142rpx;
  height: 52rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #bbbbbb;
  color: #bbbbbb;
}

.rule{
  margin-top: 30rpx;
  margin-left: 30rpx;
  /*margin-bottom: 150rpx;*/
  font-size: 22rpx;
  color: #1a1a1a;
}

.no-coupon-container{
  height: 100rpx;
  width: 690rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}