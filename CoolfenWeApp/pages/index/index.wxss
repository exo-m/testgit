.ticket-container{
    background-color: white;
    height: 330rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
}

.ticket-top-container{
    height: 100rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
}

.ticket-top{
    width: 100%;
    display: flex;
}

.ticket-top-left{
    flex: 1;
    display: flex;
    align-items: flex-end;
}

.ticket-top-right{
    flex: 1;
    display: flex;
    flex-direction: row-reverse;
    align-items: flex-end;
}

.ticket-scroll-view{
    width: 750rpx;
    height: 100rpx;
}

.ticket-item{
    margin: 0 10rpx;
    height: 100rpx;
    width: 170rpx;
    display: inline-block;
    position: relative;
}

.ticket-text{
    height: 100rpx;
    width: 170rpx;
    position:absolute;
    top: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    color: white;
}

.ticket-text-top{
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
}

.ticket-text-bottom{
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 22rpx;
    text-align: center;
}

.ticket-text-sapce{
    width: 10rpx;
}

.vouchers{
    background-image: url('data:image/png;base64,')
}

.cash-coupons{
    background-image: url('data:image/png;base64,');
}

.red-envelopes{
    background-image: url('data:image/png;base64,');
}

.discount-coupons {
    background-image: url('data:image/png;base64,');
}

.merchants-container{
    width: 100%;
    background-color: white;
}

.merchant-item-container{
    width: 720rpx;
    height: 249rpx;
    border-bottom: 1rpx #cccccc solid;
    margin-left:30rpx;
    background-color: white;
    display: flex;
}

.merchant-item-img-container{
    height: 100%;
    width: 160rpx;
    padding-top: 30rpx;
}

.merchant-item-img{
    width: 160rpx;
    height: 160rpx;
}

.merchant-item-space{
    width: 30rpx
}

.merchant-item-text-container{
    flex: 1;
    height: 100%;
    display: flex;
    flex-direction: column;
}

.merchant-item-text-title-space{
    flex: 0.8;
}

.merchant-item-text-space{
    flex: 1;
}

.merchant-item-text-tag-row{
    display: flex;
    align-items: baseline;
}
.merchant-item-text-tag-row-space{
    width: 30rpx;
}

.merchant-item-text-tag{
    padding: 0 10rpx;
    height: 34rpx;
    background-color: #fde4ee;
    border-radius: 2rpx;
    display: flex;
    align-items: center;
    justify-content: center;

}

.merchant-item-text-price{
}

.merchant-item-text-price-sapce{
    width: 12rpx;
}

.merchant-item-text-circle{
    width: 32rpx;
    height: 32rpx;
    border-radius: 50rpx;
    color: white;
    font-weight: 500;
    display: flex;
    justify-content: center;
    align-items: center;
}

.merchant-item-text-circle-sapce{
    width: 10rpx;
}

.red-circle{
    background-color:#f44556;
}

.yellow-circle{
    background-color:#ffb900;
}

.merchant-item-text-point{
    display: flex;
    align-items: center;
}

.merchant-item-text-rule-space{
    height: 16rpx;
}

.loadmore{
    margin-top: 30rpx;
    width: 100%;
    height: 70rpx;
    background-color: white;
    color: #f77a2d;
    font-size: 24rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}




