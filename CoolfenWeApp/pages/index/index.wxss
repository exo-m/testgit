page{
    height: 100%;
    background-color: #f3f3f3;
}

.page-container{
    height: 100%;
    width: 100%;
}

.header{
    width: 100%;
    height: 370rpx;
    background-color: white;
    display: flex;
}
.header-left{
    width: 50rpx;
}

.header-right{
    flex: 1;
    display: flex;
    flex-direction: column;
}

.header-top{
    height: 190rpx;
    border-bottom: 1rpx #f77a2d solid;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
}

.header-bottom{
    flex: 1;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.header-sapce{
    /*height: 40rpx;*/
    flex: 1;
}

.header-top-content{
    height: 110rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-start;
}

.header-sapce-gap{
    flex: 1;
}


.header-money{
    vertical-align: bottom;
}

.header-bottom-money{
    flex: 1;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.header-bottom-money-row-space{
    height: 13rpx;
}

.header-bottom-money-col-space{
    width: 10rpx;
}

